{
  "expo": {
    "name": "Word Hunt",
    "slug": "Word Hunt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wh_logo_small.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/wh_logo_small.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wh_logo_small.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wh_logo_small.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2234626467432384~9208473542", 
          "iosAppId": "ca-app-pub-XXXXXXXXXXXX~XXXXXXXXXX" 
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
